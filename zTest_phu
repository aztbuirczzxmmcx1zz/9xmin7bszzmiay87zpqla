	end
	if not isfile or not isfile(FILE_NAMEHLtotal) then
		writefile(FILE_NAMEHLtotal, "0")
	end

	if _G.TotalRate ~= nil then
		writefile(FILE_NAMEHLtotal, tostring(_G.TotalRate))
	end
end)

