local FILE_NAMEHL = "HLtotal.txt"
if not readfile then
    return warn("readfile không được hỗ trợ trong môi trường này.")
end
if not isfile or not isfile(FILE_NAMEHL) then
    writefile(FILE_NAMEHL, "0")
end
local content = readfile(FILE_NAMEHL)
local val = tonumber(content)
if val == nil then
    val = 51000
    writefile(FILE_NAMEHL, "51000")
end
_G.TotalHLGet = val


local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Save = require(ReplicatedStorage.Library.Client.Save)
local function safeFetchSave()
    for i = 1, 3 do
        local ok, data = pcall(Save.Get)
        if ok and data and data.TimeTrialStats then
            return data.TimeTrialStats
        end
        task.wait(1)
    end
    return { DailyRuns = 0, Points = 0 }
end
local function getRunsLeft()
    local stats = safeFetchSave()
    return 10 - (stats.DailyRuns or 0)
end
_G.RunsLeft_TIMEtritral = getRunsLeft()


setfpscap(3)
