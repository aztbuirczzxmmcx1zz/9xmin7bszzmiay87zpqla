
if name_vps == "vin3" then
	EVENT_or_FARM = "F"
	Start_ford = 2
elseif name_vps == "V5" or name_vps == "V6" or name_vps == "plz" then
	EVENT_or_FARM = "E"
	Start_ford = 1
elseif name_vps == "vin2" or name_vps == "vin4" or name_vps == "V7" then
	EVENT_or_FARM = "F"
	Start_ford = 1
else
	EVENT_or_FARM = "F"
	Start_ford = 1
end

if EVENT_or_FARM == "E" then
	getgenv().GWEBHOOK_LINK = dis_event
else
	getgenv().GWEBHOOK_LINK = dis_farm
end

