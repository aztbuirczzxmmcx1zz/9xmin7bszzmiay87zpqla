
if name_vps == "vin3" then
	EVENT_or_FARM = "E"
	Start_ford = 2
elseif name_vps == "V5" or name_vps == "V6" or name_vps == "plz" then
	EVENT_or_FARM = "E"
	Start_ford = 1
elseif name_vps == "vin2" or name_vps == "vin4" then
	EVENT_or_FARM = "F"
	Start_ford = 1
else
	EVENT_or_FARM = "E"
	Start_ford = 1
end
