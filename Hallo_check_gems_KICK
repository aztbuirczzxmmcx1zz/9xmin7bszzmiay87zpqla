local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local leaderstats = LocalPlayer:WaitForChild("leaderstats")
local diamondStat = leaderstats:WaitForChild("ðŸ’Ž Diamonds")

local function check_diamond_10min()
    local lastDiamonds = diamondStat.Value
    while task.wait(600) do
        local currentDiamonds = diamondStat.Value
        local diamondsEarned = currentDiamonds - lastDiamonds


        lastDiamonds = currentDiamonds

        if diamondsEarned < 0 then
            continue
        end

        if diamondsEarned < 1700000 then
            pcall(function()
                local Players = game:GetService("Players")
                local player = Players.LocalPlayer
                Players.LocalPlayer:Kick("Low gems")
                --loadstring(game:HttpGet("https://raw.githubusercontent.com/aztbuirczzxmmcx1zz/9xmin7bszzmiay87zpqla/refs/heads/main/Hallo_lowjoinheopro"))()
            end)
        end
    end
end

task.spawn(check_diamond_10min)
