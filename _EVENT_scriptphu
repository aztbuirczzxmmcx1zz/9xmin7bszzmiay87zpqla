task.wait(50)

local function autoShutdownIfNotAlone()
    local playerCount = #Players:GetPlayers()

    if playerCount > 1 then
        game:Shutdown()
		Players.LocalPlayer:Kick("kick")
    end
end

autoShutdownIfNotAlone()


end)

